<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <simipwa>
            <homescreen>
                <homescreen_enable>1</homescreen_enable>
                <app_name>Simicart PWA</app_name>
                <app_short_name>Simicart PWA | Progressive Web App</app_short_name>
                <home_screen_icon>https://www.simicart.com/skin/frontend/default/simicart2.0/css/2017/images/icon-logo.png</home_screen_icon>
            </homescreen>
            <notification>
                <enable>1</enable>
                <icon_url>https://www.simicart.com/skin/frontend/default/simicart2.0/css/2017/images/icon-logo.png</icon_url>
            </notification>
            <general>
                <pwa_enable>0</pwa_enable>
                <dashboard_url>https://www.simicart.com</dashboard_url>
                <pwa_mobile_only>1</pwa_mobile_only>
                <dynamic_rendering>0</dynamic_rendering>
            </general>
        </simipwa>
    </default>
</config>